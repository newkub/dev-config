    "@cli/script-cli": "workspace:*",
    "@packages/config": "workspace:*",
    "@types/node": "24.0.14",
    "typescript": "^5.8.3"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  }
}
