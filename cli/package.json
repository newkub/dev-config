    "dev": "bun run --watch src/index.ts",
    "start": "bun dist/index.js",
    "format": "biome format --write",
    "lint": "script-cli lint"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "execa": "9.6.0"
  },
  "devDependencies": {
    "@types/node": "24.7.0",
    "config": "workspace:*",
    "picocolors": "^1.1.1",
    "typescript": "^5.9.3"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  }
